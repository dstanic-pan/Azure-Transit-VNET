        <routing-table>
          <ip>
            <static-route>
              <entry name="health-probe">
                <nexthop>
                  <ip-address>10.0.2.1</ip-address>
                </nexthop>
                <bfd>
                  <profile>None</profile>
                </bfd>
                <path-monitor>
                  <enable>no</enable>
